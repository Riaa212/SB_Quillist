<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    
    <changeSet id="1" author="devlopers">
    <createTable tableName="product">
    <column name="id" type="INTEGER"></column>
    <column name="name" type="varchar(20)"></column>
    </createTable> 
    </changeSet>
    
    <changeSet id="2" author="devloper2">
    <createTable tableName="student">
    <column name="id" type="INTEGER">
    <constraints primaryKey="true" />
    </column>
    <column name="name" type="varchar(20)"></column>
    <column name="email" type="varchar(30)"></column>
    <column name="city" type="varchar(30)"></column>
    </createTable>
    </changeSet>
</databaseChangeLog>